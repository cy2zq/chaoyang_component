{
  "include": ["src", "tests", ".dumirc.ts", "*.ts", "docs"],
  "compilerOptions": {
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "types": ["vitest/globals"],
    "noImplicitAny": false,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src"],
      "chaoyang_component": ["src"],
      "chaoyang_component/*": ["src/*", "*"]
    }
  }
}
